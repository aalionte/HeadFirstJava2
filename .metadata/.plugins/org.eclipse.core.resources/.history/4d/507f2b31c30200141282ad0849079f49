package ch12;

import java.awt.*;
import javax.swing.*;

public class MyDrawPanel extends JPanel {
	
	public void repaint(){
		
	}
	
	public void paintComponent(Graphics g){
		g.setColor(Color.white);
		g.fillRect(0, 0, this.getWidth(), this.getHeight());
		g.setColor(Color.orange);
		g.fillRect(20, 50, 100, 50);
	}
}
