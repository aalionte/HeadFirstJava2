package ch15;

public class MyRunnable  implements Runnable{

	public void run() {
		// TODO Auto-generated method stub
		
	}
	
	public void go(){
		doMore();		
	}

	private void doMore() {
		System.out.println("Top");		
	}	

}

class ThreadTester{
	public static void main(String[] args){
		Runnable threadJob = new MyRunnable();
	}
}
